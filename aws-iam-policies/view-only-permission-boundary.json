        "access-analyzer:List*",
        "access-analyzer:ValidatePolicy",
        "trustedadvisor:Describe*",
        "trustedadvisor:Get*",
        "tag:Get*",
        "resource-groups:List*",
        "resource-groups:Get*",
        "resourcegroupstaggingapi:Get*",
        "pricing:GetProducts",
        "pricing:DescribeServices",
        "pricing:GetAttributeValues",
        "ce:Get*",
        "ce:Describe*",
        "support:Describe*",
        "health:Describe*",
        "backup:List*",
        "backup:Describe*",
        "backup:Get*",
        "ssm:List*",
        "ssm:Get*",
        "ssm:Describe*",
        "events:List*",
        "events:Describe*",
        "states:List*",
        "states:Describe*",
        "states:Get*",
