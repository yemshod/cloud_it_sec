        "kms:List*",
        "kms:Describe*",
        "kms:Get*",
        "cloudfront:List*",
        "cloudfront:Get*",
        "route53:List*",
        "route53:Get*",
        "route53:TestDNSAnswer",
        "apigateway:GET",
