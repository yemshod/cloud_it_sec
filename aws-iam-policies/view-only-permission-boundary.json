        "acm:Get*",
        "waf:List*",
        "waf:Get*",
        "wafv2:List*",
        "wafv2:Get*",
        "wafv2:Describe*",
        "shield:List*",
        "shield:Describe*",
        "shield:Get*",
        "directconnect:Describe*",
        "globalaccelerator:List*",
        "globalaccelerator:Describe*",
        "networkmanager:Get*",
        "networkmanager:List*",
        "networkmanager:Describe*",
        "vpc-lattice:List*",
        "vpc-lattice:Get*",
        "network-firewall:List*",
        "network-firewall:Describe*",
        "secretsmanager:List*",
        "secretsmanager:Describe*",
        "ram:List*",
        "ram:Get*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Deny",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::*-confidential-*/*",
        "arn:aws:s3:::*-sensitive-*/*",
        "arn:aws:s3:::*-pii-*/*",
        "arn:aws:s3:::*-financial-*/*"
      ]
    },
    {
      "Effect": "Deny",
      "Action": [
        "secretsmanager:GetSecretValue",
        "ssm:GetParameter*",
        "kms:Decrypt",
        "lambda:GetFunction",
        "dynamodb:GetItem",
        "dynamodb:BatchGetItem",
        "dynamodb:Query",
        "dynamodb:Scan",
        "rds:DownloadDBLogFilePortion"
      ],
      "Resource": "*"
    }
  ]
}
